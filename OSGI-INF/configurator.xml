<?xml version="1.0"?>
<scr:component name="configurator" xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0">
	<implementation class="org.protege.owl.server.configuration.ConfiguratorImpl"/>
	<service>
		<provide interface="org.protege.owl.server.api.Configurator"/>
	</service>	
	<reference name="FACTORY"
		interface="org.protege.owl.server.api.ServerFactory"
		bind="addServerFactory"
		unbind="removeServerFactory"
		cardinality="0..n"
		policy="dynamic"/>
</scr:component>
